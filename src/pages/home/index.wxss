:local(.root) {
  height: 100%;
}
:local(.root) .trigger {
  font-size: 20px;
  line-height: 64px;
  padding: 0 24px;
  cursor: pointer;
  transition: color .3s;
}
:local(.root) .ant-layout-header {
  -webkit-box-shadow: 0 8px 20px 0 rgba(40, 37, 89, 0.6);
  box-shadow: 0 8px 20px 0 rgba(40, 37, 89, 0.6);
  height: 4.75rem;
  padding: 1.25rem;
  background: #3d3780;
  color: #fff;
  display: flex;
  flex-direction: row;
  display: -webkit-flex;
  align-items: center;
}
:local(.root) .ant-layout-sider {
  background: #3d3780;
  -webkit-box-shadow: 0 8px 20px 0 rgba(40, 37, 89, 0.6);
  box-shadow: 0 8px 20px 0 rgba(40, 37, 89, 0.6);
}
:local(.root) .ant-menu-dark,
:local(.root) .ant-menu-dark .ant-menu-sub {
  background: #3d3780;
}
:local(.root) .ant-menu-dark .ant-menu-inline.ant-menu-sub {
  background-color: #2f296b;
}
:local(.root) .logo {
  display: inline-block;
  height: 32px;
  margin: 16px;
  line-height: 32px;
  text-align: center;
  font-size: 20px;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
}
:local(.root) .menuSmall {
  margin-top: 2em;
}
:local(.root) .content {
  margin: 2em;
}
:local(.root) .ant-layout {
  background-color: #2f296b;
}
:local(.root) .suportBtn {
  margin-top: -1em;
  margin-bottom: 1em;
  text-align: center;
}
:local(.root) .suportBtn .btn {
  flex-direction: row;
  display: -webkit-flex;
  align-items: center;
  margin: auto;
  padding: .55rem 1rem;
}
:local(.root) .suportBtn .suportIcon {
  font-size: 30px;
  margin-right: 0.2em;
}
